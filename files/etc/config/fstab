
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '0'
	option auto_mount '0'
	option delay_root '5'
	option check_fs '0'

config mount
	option enabled '1'
	option target '/boot'
	option label 'BOOT'
	option fstype 'vfat'
	option enabled_fsck '1'

config mount
	option enabled '1'
	option target '/rom'
	option uuid '80efdb00-0f7d-4533-befa-e6cb5b00e678'
	option fstype 'btrfs'
	option options 'compress=zstd:6'
	option enabled_fsck '1'

